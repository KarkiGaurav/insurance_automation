{
  "name": "insurance-form-automation",
  "version": "1.0.0",
  "description": "Production-ready Auto Insurance Form Automation API with Puppeteer",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "docker:build": "docker build -t insurance-automation .",
    "docker:run": "docker run -p 3000:3000 insurance-automation",
    "docker:compose": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "setup": "npm install && cp .env.example .env",
    "clean": "rm -rf logs/* temp/screenshots/*",
    "migration:prepare": "echo 'Preparing for production migration...'"
  },
  "keywords": [
    "insurance",
    "automation",
    "puppeteer",
    "form-filling",
    "api",
    "nodejs",
    "production"
  ],
  "author": "Your Company",
  "license": "ISC",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "puppeteer": "^21.0.0",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "dotenv": "^16.3.1",
    "compression": "^1.7.4",
    "express-rate-limit": "^6.7.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.5.0",
    "supertest": "^6.3.3",
    "eslint": "^8.40.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourcompany/insurance-form-automation.git"
  },
  "bugs": {
    "url": "https://github.com/yourcompany/insurance-form-automation/issues"
  },
  "homepage": "https://github.com/yourcompany/insurance-form-automation#readme"
}